<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="SakilaPU">
        <class>model.ActorEntity</class>
        <class>model.CityEntity</class>
        <class>model.CountryEntity</class>
        <class>model.AddressEntity</class>
        <class>model.CategoryEntity</class>
        <class>model.CustomerEntity</class>
        <class>model.FilmEntity</class>
        <class>model.FilmActorEntity</class>
        <class>model.FilmCategoryEntity</class>
        <class>model.FilmTextEntity</class>
        <class>model.InventoryEntity</class>
        <class>model.LanguageEntity</class>
        <class>model.PaymentEntity</class>
        <class>model.RentalEntity</class>
        <class>model.StaffEntity</class>
        <class>model.StoreEntity</class>
        <properties>
            <property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/sakila"/>
            <property name="eclipselink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
            <property name="eclipselink.jdbc.user"/>
            <property name="eclipselink.jdbc.password"/>
            <property name="hibernate.connection.url" value="jdbc:mysql://localhost:3306/sakila"/>
            <property name="hibernate.connection.driver_class" value="com.mysql.jdbc.Driver"/>
            <property name="hibernate.connection.username"/>
            <property name="hibernate.connection.password"/>
            <property name="openjpa.ConnectionURL" value="jdbc:mysql://localhost:3306/sakila"/>
            <property name="openjpa.ConnectionDriverName" value="com.mysql.jdbc.Driver"/>
            <property name="openjpa.ConnectionUserName"/>
            <property name="openjpa.ConnectionPassword"/>
            <property name="toplink.jdbc.url" value="jdbc:mysql://localhost:3306/sakila"/>
            <property name="toplink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
            <property name="toplink.jdbc.user"/>
            <property name="toplink.jdbc.password"/>
        </properties>
    </persistence-unit>
</persistence>
